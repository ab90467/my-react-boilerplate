{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": [1, 70, 2, { "ignoreComments": true }],
        "no-console": ["error", { "allow": ["warn", "error"] }],
        "quotes": ["error", "single"]
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "env": {
        "es6": true,
        "browser": true
    }
}